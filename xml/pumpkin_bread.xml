  <recipe title="Pumpkin Bread">
    <source>Doris Holland</source>
    <ingredients>
      <ingredient name="sugar">
    	<measure qty="3" unit="cup"/>
      </ingredient>
      <ingredient name="egg">
    	<measure qty="4"/>
      </ingredient>
      <ingredient name="water">
    	<measure qty="2/3" unit="cup"/>
      </ingredient>
      <ingredient name="vegetable oil">
    	<measure qty="1" unit="cup"/>
      </ingredient>
      <ingredient name="pumpkin">
    	<measure qty="2" unit="cup"/>
      </ingredient>
      <ingredient name="flour">
    	<measure qty="3 1/2" unit="cup"/>
      </ingredient>
      <ingredient name="baking soda">
    	<measure qty="2" unit="teaspooon"/>
      </ingredient>
      <ingredient name="baking powder">
    	<measure qty="1/2" unit="teaspoon"/>
      </ingredient>
      <ingredient name="salt">
    	<measure qty="1" unit="teaspoon"/>
      </ingredient>
      <ingredient name="cinnamon">
    	<measure qty="1 1/2" unit="teaspoon"/>
      </ingredient>
      <ingredient name="walnuts" form="chopped">
        <measure-range>
    	  <min-measure qty="1" unit="cup"/>
    	  <max-measure qty="1 1/2" unit="cup"/>
        </measure-range>
      </ingredient>
    </ingredients>
    <steps>
      <step>Combine sugar, eggs, water, vegetable oil, and pumpkin.</step>
      <step>Add remaining ingredients and mix well.</step>
      <step>Bake at 350Â°F for 90 minutes.</step>
    </steps>
  </recipe>
